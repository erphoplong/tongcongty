                        <table>
                            <tr>
                                <th><h1>Product</h1></th>
                                <th>
                                    <a href="~/Content/Files/IMPORT_HANGHOA.xlsx" class="fa fa-file-excel-o" > Get sample file</a>
                                </th>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    <hr />
                                    <input type="file" name="UploadedFile" class="file" value="Choose File From Your Computer">
